<p id="notice"><%= notice %></p>

<h1>Ensamblajes</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ensamblajes.each do |ensamblaje| %>
      <tr>
        <td><%= link_to 'Show', ensamblaje %></td>
        <td><%= link_to 'Edit', edit_ensamblaje_path(ensamblaje) %></td>
        <td><%= link_to 'Destroy', ensamblaje, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ensamblaje', new_ensamblaje_path %>
